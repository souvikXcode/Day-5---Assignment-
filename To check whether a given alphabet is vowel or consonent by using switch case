#include <stdio.h>

int main() {
    char ch;
    printf("Enter an alphabet: ");
    scanf(" %c", &ch); 
    if (ch >= 'A' && ch <= 'Z') {
        ch = ch + 32;  
    }
    switch(ch) {
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
            printf("'%c' is a Vowel.\n", ch);
            break;

        default:
            if ((ch >= 'a' && ch <= 'z')) {
                printf("'%c' is a Consonant.\n", ch);
            } else {
                printf("'%c' is not a valid alphabet.\n", ch);
            }
    }

    return 0;
}
